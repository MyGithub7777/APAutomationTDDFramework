<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegessionSuite">

	<listeners>
			<listener class-name = "com.odoo.ap.utility.ListenerSetup"/>
			<listener class-name = "com.odoo.ap.utility.ExtentReporterNG"/>
	</listeners>

  <test thread-count="5" name="RegressionTest">
    <classes>
      <class name="com.odoo.ap.testlayer.LoginTest"/>
      <class name="com.odoo.ap.testlayer.AddressTest"/>
      <class name="com.odoo.ap.testlayer.SignupTest"/>
      <class name="com.odoo.ap.testlayer.ProductTest"/>
    </classes>
  </test> <!-- RegressionTest -->
</suite> <!-- RegessionSuite -->
